dml 1.2;
device test;
import "io-memory.dml";

/// COMPILE-ONLY

bank b {
    parameter register_size = 4;
    register r[4] {
        parameter offset = $i * 4;
        method set(value) {
            $q[$index].f1 = value;
        }
    }
    register q[4] @undefined {
        field f1 [5:0];
    }
