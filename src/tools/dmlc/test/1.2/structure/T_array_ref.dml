dml 1.2;
device test;

/// COMPILE-ONLY

// bug 9825

bank a {
    parameter register_size = 4;
    parameter friend_bank default $b;

    group g[e in 0..0] {
        register foo @ 0 {
            method read() -> (value) {
                inline $friend_bank.foo.get() -> (value);
            }
        }
        register bar[f in 0..1][g in 0..1] @ $f * 8 + $g * 4 + 4 {
            method read() -> (value) {
                inline $friend_bank.foo.get() -> (value);
            }
        }
    }
}

bank b {
    parameter register_size = 4;
    register foo @ 0;
}
