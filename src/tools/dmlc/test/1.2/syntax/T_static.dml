dml 1.2;
device test;
import "testing.dml";

method m(int n) {
    static int x = 17;
    x++;
    assert x == 17 + n;
}

method test -> (bool ok) {
    call $m(1);
    call $m(2);
    call $m(3);
    call $m(4);

    inline $m(1);
    inline $m(1);
    inline $m(1);
    inline $m(1);

    ok = true;
}
