dml 1.2;
device test;
import "testing.dml";

method test -> (bool ok) {
    local uint32 x = 1234;
    ok = true;
    ok = ok && ((x  = 15) == 15);
    ok = ok && ((x += 15) == 30);
    ok = ok && ((x &= 15) == 14);
    ok = ok && ((x[5:2] = 3) == 3);
}
