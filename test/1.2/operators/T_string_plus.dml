dml 1.2;
device test;
import "testing.dml";

parameter foo = "hej" + " hopp";
parameter bar = $foo + " i lingonskogen";

method test -> (bool ok) {
    local const char *s = $bar + "!";

    ok = (strcmp(s, "hej hopp i lingonskogen!") == 0);
}
