dml 1.2;
device test;
import "testing.dml";

method f(v) -> (bool b) {
    b = defined v ? v == 4711 : true;
}

method test -> (bool ok) {
    inline $f(undefined) -> (ok);
}

bank b {
    group g[2] {
        register r[2] size 1 @ undefined;
    }
}

parameter u = undefined;
parameter uu = $u;
parameter gs = $b.g;
parameter g = $b.g[0];
parameter r = $b.g[1].r[0];

method init() {
    if (defined $uu) { error; }
    if (!defined $gs) { error; }
    if (!defined $g) { error; }
    if (!defined $r) { error; }
}
