dml 1.2;

device test;

/// COMPILE-ONLY

template inferior_x {
    method x() default {}
}

template inferior_y {
    parameter y default undefined;
}

template superior {
// PORT-DML-ERROR EPOVERRIDE
    register r size 1 @ 0;
// PORT-DML-ERROR EPOVERRIDE
    register r {
        is inferior_y;
        // no error in 1.2
        method x() {}
    }
}

bank xyz is superior {
    register r is inferior_x;
}
