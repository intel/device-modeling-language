dml 1.2;
device test;
/// COMPILE-ONLY

import "io-memory.dml";

bank b {
    parameter register_size = 4;
    register r[i in 0..3][j in 0..3] {
        parameter offset = 16 * $i + $j * 4;
        method set(value) {
            $q[$i][$j] = value;
        }
    }
    register q[i in 0..3][j in 0..3] @undefined;
}
