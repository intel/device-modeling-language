dml 1.2;
device test;

import "utility.dml";
import "io-memory.dml";

attribute read_raised { parameter allocate_type = "bool"; }
attribute write_raised { parameter allocate_type = "bool"; }

bank inquiry {
    register r size 4 @ 0x0 {
        method read() -> (value) {
            $read_raised = true;
        }
        method write(value) {
            $write_raised = true;
        }
    }
}
