dml 1.2;
device test;
import "testing.dml";

struct S {
    const char *s;
}

struct A {
    struct {
        const char *str;
    } u;
}


method test -> (bool ok)
{
    local const char *s1 = "hej";
    local const char *s2;
    local S x;
    local A a;
    s2 = s1;
    s1 = "hopp";
    x.s = s1;
    s2 = x.s;
    a.u.str = s1;
    s2 = a.u.str;

    ok = true;
}
