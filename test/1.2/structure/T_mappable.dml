dml 1.2;
device test;
import "testing.dml";

bank b1 {
    parameter mappable = true;
}

bank b2 {
    parameter mappable = false;
}

bank b3 {
}

bank {
    register r size 4 @ 0;
}

method test -> (bool ok)
{
    ok = ($r.parent.mappable == false
          && $b3.mappable);
}
