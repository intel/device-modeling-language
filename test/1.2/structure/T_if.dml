dml 1.2;
device test;
import "testing.dml";

parameter A = true;

if ($A) {
    data int a;

    if ($A) {
        data int d;
    }
}

if ($A) {
    data int b;
} else {
    data int c;
}

bank B {
    if ($A) {
        data int a;
    }

    if ($A) {
        data int b;
    } else {
        data int c;
    }

    if (!$A) {
        data int d;
    } else if ($name == "B") {
        data int e;
    } else {
        data int f;
    }
}

method test -> (bool ok) {
    $a = 1;
    $b = 2;
    $d = 2;
    $B.a = 1;
    $B.b = 2;
    $B.e = 3;
    ok = true;
}
