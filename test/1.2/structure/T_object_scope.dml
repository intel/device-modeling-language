dml 1.2;
device test;
import "testing.dml";

parameter A = 1;
parameter D = 4;

bank b {
    data uint32 D;

    parameter A = 2;
    parameter B = 3;

    parameter C = $A;
    parameter E = $D;
}

method test -> (bool ok) {
    $b.D = 4711;

    ok = ($b.C == 2 &&
          $b.E == 4711);
}
