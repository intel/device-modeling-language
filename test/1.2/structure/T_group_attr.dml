dml 1.2;
device test;

// See bug 5989

bank b {
    group g {
        attribute a {
            parameter allocate_type = "uint32";
        }
    }
    group ga[i in 0..3] {
        attribute a[j in 0..2][k in 0..2] {
            parameter type = "i";
            parameter configuration = "pseudo";
            method get -> (attr_value_t val) {
                val = SIM_make_attr_uint64($i * 100 + $j * 10 + $k);
            }
        }
    }
}
