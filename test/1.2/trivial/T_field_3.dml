dml 1.2;
device test;
bitorder be;
import "testing.dml";

bank b {
    register r size 2 @ 0x0100 {
	field f [8];
    }
}

method test -> (bool result) {
    result = (($b.r.f.bitsize == 1)
	      && ($b.r.f.msb == 7) && ($b.r.f.lsb == 7));
}
