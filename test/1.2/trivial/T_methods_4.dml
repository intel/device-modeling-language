dml 1.2;
device test;
import "testing.dml";

method test -> (bool result) {
    local int i = 17;
    local int j = 42;
    call $swap(i, j) -> (i, j);
    result = ((i == 42) && (j == 17));
}

method swap(int x, int y) -> (int r1, int r2) {
    r1 = y;
    r2 = x;
}
