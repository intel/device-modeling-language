dml 1.2;

device test;

import "testing.dml";

// Test that some operators have the right precedence, bug 11935

method test() -> (bool ok) {
    local uint64 a = 1 << 32;
    local double b = 1.0;
    assert ((-a) * b) != (-(a * b));
    ok = (-a * b) == ((-a) * b);
    // precedence of unary & and binary +
    /// CC-FLAG -Wno-tautological-compare
    ok = ok && ((&a + 1) == ((&a) + 1));
}
