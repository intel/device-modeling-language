dml 1.2;
device test;
import "testing.dml";

method test -> (bool ok) {
    local int i = 123_456;
    local int b1 = 0b_1010_0101;
    local int b2 = 0b1_01__00___101;
    local int x1 = 0x_dead_beef;
    local int x2 = 0xd_ea__db___eef;
    ok = (i == 123456
          && b1 == 0b10100101 && b2 == 0b10100101
          && x1 == 0xdeadbeef && x2 == 0xdeadbeef);
}
