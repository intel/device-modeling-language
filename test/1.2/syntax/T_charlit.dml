device test;
import "testing.dml";

method test -> (bool ok) {
    local int c1 = 'a';
    local int c2 = '"';
    local int c3 = '\\';
    local int c4 = '\n';
    local int c5 = '\r';
    local int c6 = '\t';
    local int c7 = '\b';
    local int c8 = ' ';
