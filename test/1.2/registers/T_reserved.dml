dml 1.2;
device test;
import "utility.dml";

bank regs {
    parameter register_size = 4;
    parameter byte_order = "big-endian";
    register r1 @ 0 is (checkreserved) {
        parameter reserved = 0xff00;
    }
    register r2 @ 4 is (checkreserved) {
        parameter reserved = 0xff00;
        field f1[11:0];
        field f2[31:12];
    }
}
