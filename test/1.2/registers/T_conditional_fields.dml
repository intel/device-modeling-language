dml 1.2;
device test;
/// COMPILE-ONLY

// This test is common for 1.2 and 1.4

// Known as bug 18611.
parameter bar_start_bit default 4;

bank b {
    register r size 4 @ 0x0000 {
        if ($bar_start_bit == 4) {
            field f[31:4];
        } else {
            field f[31:8];
        }
    }
}
