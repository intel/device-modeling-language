device test;

/// COMPILE-ONLY

bank b {
    register r[1] size 4 @ 0x10 * $i;
    group g[g in 0..0] {
        register r1    size 4 @ 0x100 + 0x100 * $g;
        register r2[1] size 4 @ 0x100 + 0x100 * $g + 0x10 * $i + 4;
