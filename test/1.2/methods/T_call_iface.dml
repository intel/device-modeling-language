dml 1.2;
device test;
import "testing.dml";
import "simics/devs/signal.dml";

// This file tests that methods that are part of an implemented
// interface can be called internally as well.

implement signal {
    method signal_raise
    {
        log "info": "RAISE";
    }
    method signal_lower
    {
        log "info": "LOWER";
    }
}

method test -> (bool ok)
{
    call $signal.signal_raise;
    call $signal.signal_lower;
    /// GREP .* RAISE
    /// GREP .* LOWER
    ok = true;
}
