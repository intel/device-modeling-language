dml 1.2;
device test;
import "testing.dml";

// This file tests that methods that are part of an event can be
// called internally as well.

event foo {
    method event(void *data)
    {
        log "info": "CALLBACK";
    }
}

method test -> (bool ok)
{
    call $foo.callback(NULL);
    /// GREP .* CALLBACK
    ok = true;
}
