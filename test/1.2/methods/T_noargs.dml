dml 1.2;

device test;

import "testing.dml";

data int val;

method m1() nothrow {
    $val = 1;
}

method m2() {
    $val = 2;
}

method test() -> (bool ok) {
    $m1();
    ok = $val == 1;
    $m2();
    ok = ok && $val == 2;
}
