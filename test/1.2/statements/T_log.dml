dml 1.2;
device test;

// bug 7809

bank {
    parameter a = 1;
}

attribute log_stuff {
    parameter configuration = "pseudo";
    parameter type = "n";
    method set(attr_value_t a) {
        /// GREP .*<1>
        log "info": "<%d>", $a;
        /// GREP .*abc 017 etc
        log "info": "abc " + "%" + "03" + "d" + " etc", 17;
        /// GREP .*in round brackets
        log "info": ("in" + " %s " + "brackets"), "round";
        /// GREP .*CRITICAL ERROR
        log "critical": "CRITICAL ERROR";    // bug 22510
    }
}
