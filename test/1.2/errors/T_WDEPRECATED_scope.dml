dml 1.2;
device test;
import "testing.dml";

method output() -> (int i) {
    /// WARNING WDEPRECATED
    local int i;
    i = 12;
}


method input(int j) -> (int i) {
    /// WARNING WDEPRECATED
    static int j;
    i = j;
}

method test() -> (bool ok) {
    local int i;
    call $output() -> (i);
    call $input(12) -> (i);
    ok = i == 0;
}
