dml 1.2;
device test;
import "testing.dml";

typedef layout "big-endian" {
    uint64 value;
} lt;

data lt val1;

data layout "big-endian" {
    uint64 value;
} val2;


method test -> (bool ok)
{
    $val1.value = 17;
    $val2.value = 18;
    ok = ($val1.value == 17 && $val2.value == 18);
}
