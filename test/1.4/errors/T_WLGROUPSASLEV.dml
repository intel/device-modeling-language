dml 1.4;
device test;

/// COMPILE-ONLY

loggroup lg;

session bool b;
session int i;

method init() {
    /// WARNING WLGROUPSASLEV
    log info, lg: "log";
    /// WARNING WLGROUPSASLEV
    log info, lg, i: "log";
    /// WARNING WLGROUPSASLEV
    log info, (lg | 0): "log";
    /// WARNING WLGROUPSASLEV
    log info, b ? (cast(lg | i, uint32) | i) : i: "log";
    /// WARNING WLGROUPSASLEV
    log info, b ? (cast(i | i, uint32) | i) : 0: "log";

    // no warning
    log info, b ? (cast(1 | i, uint32) | i) : i: "log";
    log info, lg + 0: "log";
    local uint64 lg_ = lg;
    log info, lg_: "log";
}
