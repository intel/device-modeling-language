dml 1.4;
device test;

/// API-VERSION 7

import "modern_attributes.dml";

attribute dict_attr is read_only_attr "some dict attr" {
    param type = "D";
    method get() -> (attr_value_t) {
        return SIM_alloc_attr_dict(0);
    }
}

method init() {
    default();
    assert SIM_attr_is_dict((SIM_get_attribute(dev.obj, "dict_attr")));
}
