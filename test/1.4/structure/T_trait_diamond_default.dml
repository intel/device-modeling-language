dml 1.4;
device test;

template AA {
    shared method b() default {
    }
    shared method c() default {
    }
}

template BB is AA {
    /// GREP .*BB
    shared method from_b_vtable() default { log info: "BB"; }
    shared method b() default {
        // this call requires that we have a well-formed vtable
        from_b_vtable();
    }
}

template CC is AA {
    /// GREP .*CC
    shared method from_c_vtable() default { log info: "CC"; }
    shared method c() default {
        // this call requires that we have a well-formed vtable
        from_c_vtable();
    }
}

template t1 {
    shared method m();
}

template t2 is t1 {
    shared method m() default {}
}

template t3 is t1 {
    shared method m() default {}
}

template t4 is t3 {
    method m() default {}
}

// Used to ICE (SIMICS-23289)
group g is (t2, t4) {
    method m() default {}
}

port x is (BB, CC) {
    method b() {
        default();
    }
    method c() {
        default();
    }
}
port y is (BB, CC);
method init() {
    x.b();
    x.c();
}
