dml 1.4;

/// INSTANTIATE-MANUALLY
device test;

// vtable size: 8 bytes
template t {
    shared method m() default {}
}

// This large number of elements takes no space, since we no longer create one
// vtable per instance.
group g[i < 2000000000] is t;

// prevent DCE of vtable
method init() {
    cast(g[0], t).m();
}
