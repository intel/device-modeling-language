dml 1.4;
device test;

import "testing.dml";

method test() throws {
       local uint8 a[2];
       local uint8* aref = &a[0];
       assert aref == &a[0];
       aref++;
       assert aref == &a[1];
       aref--;
       assert aref == &a[0];
}
