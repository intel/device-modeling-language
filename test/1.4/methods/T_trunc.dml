dml 1.4;

device test;

import "testing.dml";

method a(int5 i) -> (int64) throws {
    return i;
}

method b(int64 i) -> (int5) throws {
    return i;
}

method c(int5 i) -> (int64) {
    return i;
}

method d(int64 i) -> (int5) {
    return i;
}

method test() throws {
    local int64 i;
    i = a(0x5f);
    expect(i == -1, "a");
    i = b(0x5f);
    expect(i == -1, "b");
    i = c(0x5f);
    expect(i == -1, "c");
    i = d(0x5f);
    expect(i == -1, "d");
}
