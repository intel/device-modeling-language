dml 1.4;

device test;

method a(int5 i) -> (int64) throws {
    return i;
}

method b(int64 i) -> (int5) throws {
    return i;
}

method c(int5 i) -> (int64) {
    return i;
}

method d(int64 i) -> (int5) {
    return i;
}

method init() {
    local int64 i;
    try {
        i = a(0x5f);
    } catch assert false;
    assert i == -1;
    try {
        i = b(0x5f);
    } catch assert false;
    assert i == -1;
    i = c(0x5f);
    assert i == -1;
    i = d(0x5f);
    assert i == -1;
}
