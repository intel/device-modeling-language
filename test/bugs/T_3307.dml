dml 1.2;
device test;
import "testing.dml";
import "io-memory.dml";
bank b {
    register r size 4 @ 0x100000000 {
        method read -> (val) {
            val = 0xdeadbeef;
        }
    }
}

method test -> (bool result) {
    local generic_transaction_t memop; // this isn't actually used
    local uint64 val;
    local uint64 x = 0x100000000;
    call $b.read_access(&memop, x, 4) -> (result, val);
    if (val != 0xdeadbeef)
        result = false;
}
