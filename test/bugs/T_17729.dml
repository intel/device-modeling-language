dml 1.2;
device test;

bank b {
    parameter partial = false;
    group g[2] {
        register r[j in 0..1] size 1 @ $i*2+$j*5;
    }
}

method init {
    local int i;
    local int j;
    for (i = 0; i < 2; i++) {
        for (j = 0; j < 2; j++) {
            log "info": "foo %d %d: %d", i, j, $b.g[i].r[j].offset;
        }
    }
}
