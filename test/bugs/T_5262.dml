dml 1.2;
device test;
/// COMPILE-ONLY

bank foo {
    register a size 4 @ 0x0;
    register b size 4 @ 0x100;
}

method post_init() {
    local int i;
    local uint64 val;

    for (i = 0; i < 64; i++) {
        inline $foo.get(i*4, 4) -> (val);
    }
}
