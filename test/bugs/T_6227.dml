dml 1.2;
device test;
import "testing.dml";

extern void foo(uint8 x[123]);

extern typedef uint8 my_uint8;

method test -> (bool ok)
{
    local uint8 y1[123];
    local uint8 y2[12];
    local my_uint8 y3[123];
    local my_uint8 y4[12];
    foo(y1);
    foo(y2);
    foo(y3);
    foo(y4);
    ok = true;
}

header %{
void foo(uint8 x[123]);
typedef uint8 my_uint8;
%}

footer %{
void foo(uint8 x[123]) { }
%}
