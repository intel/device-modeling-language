dml 1.2;
device test;
import "io-memory.dml";

struct s {
    int m;
}

bank b1 {
    parameter function = 0;
    register r size 4 @ 0 {
        method write(val) {
            /// ERROR EPTYPE
            call $m(val);
        }
    }
}

method m(s p) {
    log "info" : "p.m=%d", p.m;
}
